    "# counting the position played by most of the squad\n",
    "query = ''' \n",
    "              SELECT\n",
    "              position,\n",
    "              COUNT(player_id)\n",
    "              FROM\n",
    "              squad\n",
    "              GROUP BY\n",
    "              position\n",
    "              ORDER BY\n",
    "              count DESC;\n",
    "        '''\n",
    "queryResult(query)"
   ]
